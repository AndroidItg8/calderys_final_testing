<?xml version="1.0" encoding="utf-8"?>
        

    

                 
                                 
                                                                                                      
           

    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:tag="layout/item_rv_cosignee_item_0"    
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true"
        app:contentPadding="@dimen/margin_4" >

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/lbl_consignee_code"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:text="Consignee Code"
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/txt_consignee_code"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:tag="binding_1"                             
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/lbl_consignee_model"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:text="Consignee Name"
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/txt_consignee_model"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:tag="binding_2"                             
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/lbl_consignee_address"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:text="Consignee Address"
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/txt_consignee_address"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:tag="binding_3"                                
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/lbl_consignee_eccno"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:text="Consignee ECCNo"
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/txt_consignee_eccNo"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:tag="binding_4"                              
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/lbl_consignee_cstNo"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:text="Consignee CSTNo"
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/txt_consignee_cstNo"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:tag="binding_5"                              
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/lbl_consignee_TinNo"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:text="Consignee TinNo"
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />

                <TextView
                    android:id="@+id/txt_consignee_tinNo"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/cust_txt"
                    android:fontFamily="@font/open_sans"
                    android:gravity="start|center_vertical"
                    android:padding="@dimen/margin_8"
                    android:tag="binding_6"                              
                    android:textColor="@color/colorGray"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="normal" />
            </TableRow>

        </TableLayout>


    </android.support.v7.widget.CardView>
         